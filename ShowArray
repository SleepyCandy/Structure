package array;
import java.util.ArrayList;

public class ShowArray{
	public void printArrayD1(ArrayList<Integer> a){	//ยืดหยุ่น
		for(int i =0;i<a.size();i++)
			System.out.print(a.get(i)+"\t");
	System.out.println();
	}
public void printArray1(int[] a){	//หนิงมิติ
		for(int i =0;i<a.length;i++)
			System.out.print(a[i]+"\t");
	System.out.println();
	}
	
public void printArray2(int[][] a){ //สองมิติ
	for(int i =0;i<a.length;i++){
		for(int j = 0; j<a[i].length; j++){
		System.out.print(a[i][j]+"\t");
		}
	}
	System.out.println();
	}
}
